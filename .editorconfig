# javascript
[*.{js,jsx,ts,tsx}]
indent_style = tab
indent_size = 4
charset = utf-8
end_of_line = crlf
trim_trailing_whitespace = true
insert_final_newline = true

# less
[*.less]
indent_style = tab
indent_size = 4
charset = utf-8
end_of_line = crlf
trim_trailing_whitespace = true
insert_final_newline = true
